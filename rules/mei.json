[
  {
    "name": "Posición y orientación local",
    "description": "La posición local del edificio o infraestructura debe estar coordinada y cercana al origen.",
    "formula": "count(#p0) = 1",
    "buildingTypes": ["HOUSE", "APARTMENT"],
    "filters": [
      {
        "spaces": [],
        "entities": ["IfcBuildingElementProxy"],
        "constraints": [
          {
            "on": "ENTITY",
            "type": "ATTRIBUTE",
            "attribute": "Name",
            "operation": "EQUAL",
            "values": ["ORIGEN"]
          },
          {
            "on": "ENTITY",
            "type": "LOCATION",
            "attribute": "x",
            "operation": "EQUAL",
            "values": [0]
          },
          {
            "on": "ENTITY",
            "type": "LOCATION",
            "attribute": "y",
            "operation": "EQUAL",
            "values": [0]
          },
          {
            "on": "ENTITY",
            "type": "LOCATION",
            "attribute": "z",
            "operation": "EQUAL",
            "values": [0]
          }
        ]
      }
    ]
  },
  {
    "name": "Niveles del proyecto y su denominación",
    "description": "Todos los niveles de un proyecto están clasificados numéricamente y con una descripción textual.",
    "formula": "#p0 = #p1",
    "buildingTypes": ["HOUSE", "APARTMENT"],
    "filters": [
      {
        "spaces": [],
        "entities": ["IfcBuildingStorey"],
        "constraints": [
          {
            "on": "ENTITY",
            "type": "ATTRIBUTE",
            "attribute": "Name",
            "operation": "EQUAL",
            "values": ["\\d\\d-\\w+"]
          }
        ]
      },
      {
        "spaces": [],
        "entities": ["IfcBuildingStorey"],
        "constraints": [
          {
            "on": "ENTITY",
            "type": "ATTRIBUTE",
            "attribute": "GlobalId",
            "operation": "EXISTS"
          }
        ]
      }
    ]
  },
  {
    "name": "Uso correcto de las entidades",
    "description": "Las entidades IFC cuentan con la clase apropiada.",
    "formula": "count(#p0) = 1",
    "buildingTypes": ["HOUSE", "APARTMENT"],
    "filters": [
      {
        "spaces": [],
        "entities": ["IfcBuildingElementProxy"],
        "constraints": [
          {
            "on": "ENTITY",
            "type": "ATTRIBUTE",
            "attribute": "GlobalId",
            "operation": "EXISTS"
          }
        ]
      }
    ]
  },
  {
    "name": "Estructura y denominación",
    "description": "Los objetos tienen correctamente asociado su tipo.",
    "formula": "#p0 \\ (#p1 | #p2 | #p3) = empty",
    "buildingTypes": ["HOUSE", "APARTMENT"],
    "filters": [
      {
        "spaces": [],
        "entities": ["IfcObject"],
        "excluded": ["IfcMember", "IfcOpeningElement"],
        "constraints": [
          {
            "on": "ENTITY",
            "type": "ATTRIBUTE",
            "attribute": "GlobalId",
            "operation": "EXISTS"
          }
        ]
      },
      {
        "spaces": [],
        "entities": ["IfcObject"],
        "excluded": ["IfcMember", "IfcOpeningElement"],
        "constraints": [
          {
            "on": "ENTITY",
            "type": "ATTRIBUTE",
            "attribute": "PredefinedType",
            "operation": "EQUAL",
            "values": ["USERDEFINED"]
          },
          {
            "on": "ENTITY",
            "type": "ATTRIBUTE",
            "attribute": "ObjectType",
            "operation": "EXISTS"
          }
        ]
      },
      {
        "spaces": [],
        "entities": ["IfcObject"],
        "excluded": ["IfcMember", "IfcOpeningElement"],
        "constraints": [
          {
            "on": "ENTITY",
            "type": "ATTRIBUTE",
            "attribute": "PredefinedType",
            "operation": "NOT_EQUAL",
            "values": ["USERDEFINED"]
          }
        ]
      },
      {
        "spaces": [],
        "entities": ["IfcObject"],
        "excluded": ["IfcMember", "IfcOpeningElement"],
        "constraints": [
          {
            "on": "TYPE",
            "type": "ATTRIBUTE",
            "attribute": "GlobalId",
            "operation": "EXISTS"
          }
        ]
      }
    ]
  },
  {
    "name": "Sistema de Clasificación",
    "description": "Todos los objetos cuentan con un sistema de clasificación asociado.",
    "formula": "#p0 <= #p1[RelatedObjects]",
    "buildingTypes": ["HOUSE", "APARTMENT"],
    "filters": [
      {
        "spaces": [],
        "entities": ["IfcBuildingElement"],
        "excluded": ["IfcMember"],
        "constraints": [
          {
            "on": "ENTITY",
            "type": "ATTRIBUTE",
            "attribute": "GlobalId",
            "operation": "EXISTS"
          }
        ]
      },
      {
        "spaces": [],
        "entities": ["IfcRelAssociatesClassification"],
        "constraints": [
          {
            "on": "ENTITY",
            "type": "ATTRIBUTE",
            "attribute": "RelatedObjects",
            "operation": "EXISTS"
          }
        ]
      }
    ]
  },
  {
    "name": "Indique el material correcto de los objetos",
    "description": "Los objetos cuentan con una descripción del material correspondiente.",
    "formula": "#p0 = #p1",
    "buildingTypes": ["HOUSE", "APARTMENT"],
    "filters": [
      {
        "spaces": [],
        "entities": ["IfcBuildingElement"],
        "excluded": ["IfcMember"],
        "constraints": [
          {
            "on": "ENTITY",
            "type": "PSET_QTO",
            "pset": "Pset_\\w+Common",
            "attribute": "Material",
            "operation": "EXISTS"
          }
        ]
      },
      {
        "spaces": [],
        "entities": ["IfcBuildingElement"],
        "excluded": ["IfcMember"],
        "constraints": [
          {
            "on": "ENTITY",
            "type": "ATTRIBUTE",
            "attribute": "GlobalId",
            "operation": "EXISTS"
          }
        ]
      }
    ]
  },
